<?xml version="1.0" encoding="utf-8"?>
<MasterDetailPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="RentApp.Views.Principal.User.UserMasterDetailPage"
    xmlns:pages="clr-namespace:RentApp.Views.Principal.User"
    xmlns:controls="clr-namespace:RentApp.Controls">
    <MasterDetailPage.Master>
        <pages:UserMasterDetailPageMaster x:Name="MasterPage" />
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <controls:NavigationViewPage IsShadow="True">
            <x:Arguments>
                <pages:UserMasterDetailPageDetail />
            </x:Arguments>
        </controls:NavigationViewPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>
